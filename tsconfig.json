{
  "compilerOptions": {
    "moduleResolution": "Node",
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": false,
    "preserveSymlinks": true,
    "skipLibCheck": true,
    "outDir": ".cache/",
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "inlineSources": true,
    "isolatedModules": true
  },
  "exclude": ["node_modules"]
}
